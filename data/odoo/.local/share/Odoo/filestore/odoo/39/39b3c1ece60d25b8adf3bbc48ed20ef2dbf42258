            trigger: ".o_web_studio_snackbar:not(:has(.fa-spin))",
            run: "click",
        },
        {
            trigger: ".o_form_view",
            run() {
                if (document.querySelector(".someDiv")) {
                    throw new Error("The edition of the view's arch via the xml editor failed");
                }
            },
        },
    ],
});

