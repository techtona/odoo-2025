    url: '/contactus',
    edition: true,
}, () => editContactUs([
    {
        content: 'Change the Recipient Email',
        trigger: '[data-field-name="email_to"] input',
        // TODO: remove && click body
        run: "edit test@test.test && click body",
    },
]));
